{
	"name": "Saucy Dark Yellowish Purple",
	"type": "dark",
	"colors": {
	  "editor.background": "#2b0033",
	  "editor.foreground": "#fdfdfd",
	  "activityBar.background": "#3b004a",
	  "activityBar.foreground": "#ffd84d",
	  "activityBarBadge.background": "#d4b300",
	  "sideBar.background": "#1f0026",
	  "sideBar.foreground": "#d9b3ff",
	  "sideBarTitle.foreground": "#cfa5ff",
	  "sideBarSectionHeader.background": "#3b0052",
	  "sideBarSectionHeader.foreground": "#ffd84d",
	  "statusBar.background": "#4c006a",
	  "titleBar.activeBackground": "#3b0052",
	  "titleBar.activeForeground": "#ffe97f",
	  "editorCursor.foreground": "#020201",
	  "editorLineNumber.foreground": "#aa73ff",
	  "editor.selectionBackground": "#ffd63344",
	  "editor.inactiveSelectionBackground": "#ffd63322"
	},
	"tokenColors": [
	  {
		"name": "Comment",
		"scope": ["comment", "punctuation.definition.comment"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#a85dff"
		}
	  },
	  {
		"name": "Variables",
		"scope": ["variable", "string constant.other.placeholder"],
		"settings": {
		  "foreground": "#ff66cc"
		}
	  },
	  {
		"name": "Keyword, Storage",
		"scope": ["keyword", "storage.type", "storage.modifier"],
		"settings": {
		  "foreground": "#e6b800"
		}
	  },
	  {
		"name": "Operator, Misc",
		"scope": [
		  "keyword.control",
		  "constant.other.color",
		  "punctuation",
		  "meta.tag",
		  "punctuation.definition.tag"
		],
		"settings": {
		  "foreground": "#c770ff"
		}
	  },
	  {
		"name": "Function, Special Method",
		"scope": [
		  "entity.name.function",
		  "meta.function-call",
		  "variable.function"
		],
		"settings": {
		  "foreground": "#ffdd66"
		}
	  },
	  {
		"name": "String",
		"scope": ["string"],
		"settings": {
		  "foreground": "#d8bf5a"
		}
	  },
	  {
		"name": "Number, Constant",
		"scope": ["constant.numeric", "constant.language"],
		"settings": {
		  "foreground": "#ffb347"
		}
	  },
	  {
		"name": "Class, Support",
		"scope": ["entity.name", "support.type", "support.class"],
		"settings": {
		  "foreground": "#d1a3ff"
		}
	  },
	  {
		"name": "Attributes",
		"scope": ["entity.other.attribute-name"],
		"settings": {
		  "foreground": "#ff99cc"
		}
	  },
	  {
		"name": "Decorators",
		"scope": ["tag.decorator.js entity.name.tag.js"],
		"settings": {
		  "fontStyle": "italic",
		  "foreground": "#ff66cc"
		}
	  },
	  {
		"name": "Markdown Emphasis",
		"scope": ["markup.bold", "markup.italic"],
		"settings": {
		  "foreground": "#ff80df"
		}
	  },
	  {
		"name": "Plain JSX/HTML Text",
		"scope": [
		  "text.html.basic",
		  "text.html.markdown",
		  "meta.jsx.children",
		  "string.unquoted",
		  "source.js.embedded.html"
		],
		"settings": {
		  "foreground": "#d6b3ff"  // soft lavender for <p> inner text
		}
	  },
	  {
		"name": "Invalid",
		"scope": ["invalid", "invalid.illegal"],
		"settings": {
		  "foreground": "#ff0033"
		}
	  }
	]
  }
  